<div>
	{% if errors %}
	<h1>Errors</h1>
	<ul>
		{% for error in errors %}
		<li>{{ error }}</li>
		{% endfor %}
	</ul>
	{% endif %}
	<h1>addNote</h1>
	<form action="/addNote/addNoteValidation/" method="post">
		<label for="title">title</label>
		<input name="title" type="text" />
		<br />
		<label for="tag">tag</label>
		<input name="tag" type="text" />
		<br />
		<label for="isPublic">isPublic</label>
		<input type="radio" value="True" checked="checked" name="isPublic" /> yes
		<input type="radio" value="False" name="isPublic" /> no
		<br />
		<label  for="content">content</label>
		<br />
		<textarea cols="30" rows="10" id="contactus" name="content"></textarea>
		<br />
		<input type="submit" value="create" />
	</form>
</div>


